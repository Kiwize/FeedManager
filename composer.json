{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "pear/text_languagedetect": "^1.0",
        "donatello-za/rake-php-plus": "^1.0",
        "php-ai/php-ml": "^0.10.0",
        "donatello-za/rake-php-plus": "^1.0",
        "php-science/textrank": "^1.2"
    },
    "require-dev": {
        "laravel/telescope": "^4.14",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "composer-production": "@composer install --no-ansi --no-dev --no-interaction --no-plugins --no-progress --no-scripts --optimize-autoloader --no-dev",
        "composer-development": "@composer install --no-ansi --no-interaction --no-plugins --no-progress --no-scripts --optimize-autoloader",
        "production:build-deps-archive": [
            "rm -fr ./vendor",
            "@composer-production",
            "tar -czf composer-vendor.tar.gz ./vendor",
            "@composer install"
        ],
        "production:extract-deps-archive": [
            "rm -fr ./vendor",
            "tar -xzf composer-vendor.tar.gz ./vendor"
        ],
        "development:build-deps-archive": [
            "rm -fr ./vendor",
            "@composer-development",
            "tar -czf composer-vendor.dev.tar.gz ./vendor",
            "@composer install"
        ],
        "development:extract-deps-archive": [
            "rm -fr ./vendor",
            "tar -xzf composer-vendor.dev.tar.gz ./vendor"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test:coverage": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-html coverage",
        "test:feeddbseed": "php artisan db:seed --class=FeedSeeder"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
